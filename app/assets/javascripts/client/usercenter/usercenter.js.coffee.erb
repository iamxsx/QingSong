#= require_self
#这里的代码用于 layout 本身和 users/user_center 本身

<%
#登出的url
@logout_url = "/logout"
#登出方法
@logout_method = "get"
#登出后请后端跳转到登录页!
%>

$ ->
  $("#logout").click ->
    $.ajax(
      url: "<%= @logout_url %>"
      type: "<%= @logout_method %>"
      success: (data, status) ->
        if status == 'success'
          window.location.href = '/'
    )